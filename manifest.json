{
    "name": "Clean Columns for Facebook",
    "description": "Removes right column from Facebook UI.",
    "version": "1.0",
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://www.facebook.com/*"
            ],
            "css": [
                "mystyles.css"
            ]
        }
    ],
    "browser_action": {
        "default_title": "cleanColumn"
    },
    "manifest_version": 2
}